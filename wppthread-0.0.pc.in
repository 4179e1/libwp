prefix=@prefix@
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: wppthread-@PKG_VERSION@
Description: thread support for libwp
Requires: libwp-@PKG_VERSION@
Version: @VERSION@
Cflags: -I${includedir}/libwp-@PKG_VERSION@ -pthread
Libs: -L${libdir} -lwppthread -lpthread
